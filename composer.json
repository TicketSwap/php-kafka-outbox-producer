{
    "name": "ticketswap/kafka-outbox-producer",
    "description": "PHP library to produce kafka message using the outbox pattern",
    "type": "library",
    "autoload": {
        "psr-4": {
            "TicketSwap\\Kafka\\Outbox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TicketSwap\\Kafka\\Outbox\\": "tests/"
        }
    },
    "require": {
        "doctrine/orm": "^2.8",
        "symfony/http-kernel": "^5.2",
        "symfony/config": "^5.2",
        "symfony/dependency-injection": "^5.2",
        "ramsey/uuid": "^4.1",
        "doctrine/doctrine-bundle": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
